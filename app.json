{
  "expo": {
    "name": "ATCC Management",
    "slug": "ATCC Management",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./banner-logo-nova.png",
      "resizeMode": "contain",
      "backgroundColor": "#154fa3"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/banner-logo-nova.png",
        "backgroundColor": "#154fa3"
      }
    },
    "web": {
      "favicon": "./assets/banner-logo-nova.png"
    }
  }
}
